{
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:compiler\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "dev:compiler": "cd Compiler && node index.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "start": "npm run start:prod",
    "start:prod": "concurrently \"npm run start:server\" \"npm run start:client\" \"npm run start:compiler\"",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start",
    "start:compiler": "cd Compiler && node index.js",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install && cd ../Compiler && npm install",
    "check:deployment": "node deployment-check.js",
    "seed:db": "cd server && node seed-problems.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.34.0",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "motion": "^12.19.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0"
  }
}
